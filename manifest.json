{
  "manifest_version": 2,
  "name": "Phishing Detector",
  "version": "1.0",
  "permissions": ["<all_urls>","https://www.googleapis.com/*",
  "https://www.googleapis.com/auth/gmail.readonly"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "Phishing Detector 128p.png",
    "default_title": "Phishing Detector"
  }
}



